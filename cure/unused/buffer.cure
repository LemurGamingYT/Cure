// 'buffer' is a library to create arrays that are limited in size
use "buffer"


func main() -> int {
    // Create an int buffer with the size of 5
    buf = create_buf("int", 5)

    // Set index 0 to 25 and index 2 to 3
    buf.set(0, 25)
    buf.set(2, 3)

    print(buf)

    print(buf.get(0))
    print(buf.get(2))

    return 0
}
