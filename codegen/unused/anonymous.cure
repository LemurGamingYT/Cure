func main() -> int {
    // Create an anonymous function and assign it to a variable 'x'
    // The function takes two integers and returns an integer
    x = func(int a, int b) -> int {
        return a + b // Return the sum of the two integers
    }
    
    // Call the function 'x' with arguments 1 and 2
    print(x(1, 2))
    return 0
}
