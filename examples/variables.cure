func main() -> int {
    // Define a variable 'x'
    x = 10
    print(x) // Variables can be used in or as expressions

    // Define a constant 'y', which cannot be reassigned
    const y = 20
    print(y)
    // y = 30 // This will cause an error

    // Define a variable 'z' with an explicit type
    int z = 30
    print(z)

    return 0
}
