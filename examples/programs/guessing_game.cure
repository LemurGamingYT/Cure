func main() -> int {
    random = new Random()
    randnum = random.next_int(1, 10)
    print("Guess the number between 1 and 10")
    while true {
        num = input("number> ").parse_int()
        if num == randnum {
            print("Correct number!")
            break
        } else if num < randnum {
            print("Try higher")
        } else if num > randnum {
            print("Try lower")
        }
    }

    return 0
}
