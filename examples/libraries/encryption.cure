// 'encryption' is a library to encode and decode text, it currently only supports base64 encoding
use "encryption"


func main() -> int {
    // Input string
    data = "Hey"

    // Encode the string
    encoded = base64_encode(data)
    print(encoded) // Outputs 'SGV5' (SGV5 is the base64 encoded version of 'Hey')

    // Decode the encoded string back into 'Hey'
    print(base64_decode(encoded))
    return 0
}
