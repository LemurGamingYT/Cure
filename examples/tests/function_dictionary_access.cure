func add_one(int x) -> int {
    return x + 1
}

func add_two(int x) -> int {
    return x + 2
}

func main() -> int {
    function_map = {
        "add_one": add_one,
        "add_two": add_two
    }

    f = function_map["add_one"]
    assert(f(1) == 2)
    assert(f(2) == 3)

    f = function_map["add_two"]
    assert(f(1) == 3)
    assert(f(2) == 4)
    return 0
}
