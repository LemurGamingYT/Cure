// Make an 'add' function that can add integers and floats
// Note that you can only overload parameters and not return types

// Define the integer version
func add(int x, int y) -> int {
    return x + y
}

// Define the float version
func add(float x, float y) -> float {
    return x + y
}

func main() -> int {
    // Now simply call it and the compiler figures out the correct version
    print(add(5, 1))
    print(add(1.05, 8.27))

    return 0
}
